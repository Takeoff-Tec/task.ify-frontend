{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "start": "vite preview",
    "dprod": "docker run -p 80:80 Task.ify/frontend-prod",
    "ddev": "docker run -p 3000:3000 Task.ify/frontend-dev",
    "dbuild-dev": "docker build -f Dockerfile.dev -t Task.ify/frontend-dev .",
    "dbuild-prod": "docker build -f Dockerfile -t Task.ify/frontend-prod .",
    "dnew-dev": "npm run dbuild-dev && npm run ddev",
    "dnew-prod": "npm run dbuild-prod && npm run dprod",
    "redeploy": "heroku container:push web && heroku container:release web",
    "build-amd64": "docker buildx build --platform linux/amd64 -t Task.ify/frontend-prod . --load"
  },
  "dependencies": {
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.1",
    "@mui/styled-engine": "^5.13.2",
    "axios": "^1.4.0",
    "axios-mock-adapter": "^1.21.5",
    "enzyme": "^3.11.0",
    "jest": "^29.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fontawesome": "^1.7.1",
    "react-router-dom": "^6.14.2",
    "ts-node": "^10.9.1",
    "vite": "^4.4.8",
    "@vitejs/plugin-react": "^4.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/spotify-web-playback-sdk": "^0.1.15",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.2"
  }
}
